<% layout('/layouts/boilerplate.ejs') -%>
<body>
    <h3>All listings</h3>
    <form action="/listings/new" method="get">
        <button>Create new Listing</button>
    </form>
    <ul>
        <% for(let listing of allListing){%>
            <li><a href="/listings/<%=listing._id %>"> <%=listing.title %></li></a>
            <%}%>
    </ul>
    
</body>
